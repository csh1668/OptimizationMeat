<?xml version="1.0" encoding="UTF-8"?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
			<li>Vanilla Achievements Expanded</li>
            <li>Vanilla Cooking Expanded</li>
		</mods>

        <match Class="PatchOperationSequence">
            <success>Always</success>
            <operations>
                <li Class="PatchOperationReplace">
                    <xpath>Defs/AchievementsExpanded.AchievementDef[defName="VCE_AchievementSpam"]/tracker/includeingredient</xpath>
					<value>
						<includeingredient>Meat_Cow</includeingredient>
					</value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>Defs/AchievementsExpanded.AchievementDef[defName="VCE_AchievementRatatouille"]/tracker/includeingredient</xpath>
					<value>
						<includeingredient>Meat_Cow</includeingredient>
					</value>
                </li> 
            </operations>
        </match>
    </Operation>
</Patch>