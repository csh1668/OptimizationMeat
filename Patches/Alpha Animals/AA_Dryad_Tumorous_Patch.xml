<?xml version="1.0" encoding="UTF-8"?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
			<li>Alpha Animals</li>
		</mods>

        <match Class="PatchOperationSequence">
            <success>Always</success>
            <operations>
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="AA_Dryad_Tumorous"]/comps/li[contains(@Class, 'CompProperties_Spawner')]/thingToSpawn</xpath>
					<value>
						<thingToSpawn>Meat_Cow</thingToSpawn>
					</value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>